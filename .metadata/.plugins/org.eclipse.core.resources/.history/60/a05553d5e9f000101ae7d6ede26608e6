package polymorphism;

public class LgTV {
	public void turnOn() {
		System.out.println("LgTV ----- 전원을 킨다.");
	}
	
	public void turnOff() {
		System.out.println("LgTV ----- 전원을 끈다.");
	}
	
	public void SoundUp() {
		System.out.println("LgTV ----- 볼륨을 높인다.");
	}
	
	public void SoundDown() {
		System.out.println("LgTV ----- 볼륨을 줄인다.");
	}
}
